<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.liferay.monitoring">
	<namespace>Monitoring</namespace>
	<entity name="Metric" uuid="true" local-service="true" remote-service="true">
		<column name="metricId" type="long" primary="true" />
		<column name="metricName" type="String" />
		<column name="metricValue" type="long"/>
		<column name="createDate" type="Date" />
	</entity>
</service-builder>